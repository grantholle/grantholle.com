{% set title = 'Grant Holle' %}
{% extends "layout.njk" %}

{% block head %}
<script>
document.addEventListener('DOMContentLoaded', (event) => {
  const elements = Array.from(document.getElementsByTagName('a'))
  const handler = event => {
      const id = event.currentTarget.getAttribute('data-bg')

      if (id) {
        const bg = document.getElementById(id)
        const action = bg.classList.contains('opacity-0')
          ? 'remove'
          : 'add'

        bg.classList[action]('opacity-0')
      }
    }

  elements.forEach((el) => {
    el.addEventListener('mouseover', handler)
    el.addEventListener('mouseleave', handler)
  })
})
</script>
{% endblock %}

